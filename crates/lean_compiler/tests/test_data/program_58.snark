// test_mutable_complex_expressions
fn main() {
    mut a = 5;
    mut b = 3;

    // Complex expression on RHS
    a = a * a + b * b;  // 25 + 9 = 34
    assert a == 34;

    b = a - b * 2;      // 34 - 6 = 28
    assert b == 28;

    mut c = a + b;      // 34 + 28 = 62
    assert c == 62;

    // Multiple mutations
    c = c + 8;          // 70
    assert c == 70;

    c = c - 10;         // 60
    assert c == 60;
    return;
}
