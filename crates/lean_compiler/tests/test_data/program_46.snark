// test_mutable_nested_unrolled_loops
fn main() {
    mut total = 0;
    mut outer_sum = 0;
    for i in 0..3 unroll {
        outer_sum = outer_sum + i;
        mut inner_sum = 0;
        for j in 0..4 unroll {
            inner_sum = inner_sum + j;
            total = total + 1;
        }
        // inner_sum should be 0+1+2+3 = 6
        assert inner_sum == 6;
    }
    // outer_sum should be 0+1+2 = 3
    assert outer_sum == 3;
    // total should be 3*4 = 12
    assert total == 12;
    return;
}
