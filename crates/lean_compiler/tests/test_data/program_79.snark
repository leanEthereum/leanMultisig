// test_mutable_edge_cross_variable_assignment
fn main() {
    mut a = 10;
    mut b = 20;

    cond = 1;
    if cond == 1 {
        a = b + 1;  // a = 21
        b = a + 1;  // b = 22 (uses new a)
    } else {
        b = a + 100;
        a = b + 1;
    }

    assert a == 21;
    assert b == 22;
    return;
}
