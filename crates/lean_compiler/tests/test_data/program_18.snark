// test_match
fn main() {
    for x in 0..3 unroll {
        func_match(x);
    }
    for x in 0..2 unroll {
        match x {
            0 => {
                y = 10 * (x + 8);
                z = 10 * y;
                print(z);
            }
            1 => {
                y = 10 * x;
                z = func_2(y);
                print(z);
            }
        }
    }
    return;
}

fn func_match(x) inline {
    match x {
        0 => {
            print(41);
        }
        1 => {
            y = func_1(x);
            print(y + 1);
        }
        2 => {
            y = 10 * x;
            print(y);
        }
    }
    return;
}

fn func_1(x) -> 1 {
    return x * x * x * x;
}

fn func_2(x) inline -> 1 {
    return x * x * x * x * x * x;
}
