fn main() {
    assert test_func(0) == 11;
    assert test_func(1) == 10;  // no mutation
    assert test_func(2) == 30;
    return;
}

fn test_func(sel) -> 1 {
    mut x = 10;
    match sel {
        0 => {
            x = x + 1;
        }
        1 => {
        }
        2 => {
            x = x + 20;
        }
    }
    return x;
}
