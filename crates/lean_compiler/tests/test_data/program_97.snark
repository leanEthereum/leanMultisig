fn main() {
    assert test_func(0, 0, 0) == 1000;
    assert test_func(0, 0, 1) == 1001;
    assert test_func(0, 1, 0) == 1010;
    assert test_func(1, 0, 0) == 1100;
    assert test_func(1, 1, 1) == 1111;
    return;
}

fn test_func(a, b, c) -> 1 {
    mut x = 0;
    if a == 0 {
        x = x + 1000;
        if b == 0 {
            if c == 0 {
            } else {
                x = x + 1;
            }
        } else {
            x = x + 10;
            if c == 1 {
                x = x + 1;
            }
        }
    } else {
        x = x + 1100;
        if b == 1 {
            x = x + 10;
            if c == 1 {
                x = x + 1;
            }
        }
    }
    return x;
}
