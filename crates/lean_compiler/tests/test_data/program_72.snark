// test_mutable_edge_deeply_nested_if_else
fn main() {
    mut x = 0;
    a = 1;
    b = 1;
    c = 1;
    if a == 1 {
        x = x + 1;
        if b == 1 {
            x = x + 10;
            if c == 1 {
                x = x + 100;
            } else {
                x = x + 200;
            }
        } else {
            x = x + 20;
        }
    } else {
        x = x + 1000;
    }
    // Expected: 0 + 1 + 10 + 100 = 111
    assert x == 111;
    return;
}
