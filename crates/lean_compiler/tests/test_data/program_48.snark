// test_mutable_with_const_args
const N = 5;
const M = 3;

fn main() {
    mut acc = 0;
    for i in 0..N unroll {
        acc = acc + M;
    }
    // acc = 5 * 3 = 15
    assert acc == 15;

    mut product = 1;
    for i in 0..M unroll {
        product = product * 2;
    }
    // product = 2^3 = 8
    assert product == 8;
    return;
}
