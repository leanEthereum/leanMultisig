fn main() {
    mut evens = 0;
    mut odds = 0;
    mut all = 0;

    for i in 0..6 unroll {
        all = all + 1;
        remainder = i % 2;
        if remainder == 0 {
            evens = evens + i;
        } else {
            odds = odds + i;
        }
    }

    assert evens == 6;
    assert odds == 9;
    assert all == 6;
    return;
}
