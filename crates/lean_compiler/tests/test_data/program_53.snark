// test_mutable_fibonacci_unrolled
fn main() {
    mut fib_prev = 0;
    mut fib_curr = 1;

    // Manual unroll for clarity
    temp0 = fib_curr;
    fib_curr = fib_prev + fib_curr;
    fib_prev = temp0;

    temp1 = fib_curr;
    fib_curr = fib_prev + fib_curr;
    fib_prev = temp1;

    temp2 = fib_curr;
    fib_curr = fib_prev + fib_curr;
    fib_prev = temp2;

    temp3 = fib_curr;
    fib_curr = fib_prev + fib_curr;
    fib_prev = temp3;

    temp4 = fib_curr;
    fib_curr = fib_prev + fib_curr;
    fib_prev = temp4;

    // After 5 iterations: fib = 0, 1, 1, 2, 3, 5, 8
    assert fib_curr == 8;
    assert fib_prev == 5;
    return;
}
