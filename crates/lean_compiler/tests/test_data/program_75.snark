fn main() {
    assert test_func(0, 1) == 11;
    assert test_func(0, 0) == 20;
    assert test_func(1, 1) == 110;
    assert test_func(1, 0) == 200;
    return;
}

fn test_func(selector, cond) -> 1 {
    mut x = 10;
    match selector {
        0 => {
            if cond == 1 {
                x = x + 1;
            } else {
                x = x + 10;
            }
        }
        1 => {
            if cond == 1 {
                x = x + 100;
            } else {
                x = x + 190;
            }
        }
    }
    return x;
}
