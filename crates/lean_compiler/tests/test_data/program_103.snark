// test_mutable_edge_mutation_with_array_index
fn main() {
    arr = malloc(5);
    for i in 0..5 unroll {
        arr[i] = i * 10;
    }
    // arr = [0, 10, 20, 30, 40]

    mut idx = 0;
    val1 = arr[idx];  // arr[0] = 0
    assert val1 == 0;

    idx = idx + 1;
    val2 = arr[idx];  // arr[1] = 10
    assert val2 == 10;

    idx = idx + 2;
    val3 = arr[idx];  // arr[3] = 30
    assert val3 == 30;

    return;
}
