const BIG_ENDIAN = 0;
const LITTLE_ENDIAN = 1;
fn main() {
    x = 2**20 - 1;
    a = malloc(31);
    print(a);
    hint_decompose_bits(x, a, 31, LITTLE_ENDIAN);
    for i in 0..20 {
        debug_assert a[i] == 1;
        assert a[i] == 1;
    }
    for i in 20..31 {
        assert a[i] == 0;
    }
    return;
}
