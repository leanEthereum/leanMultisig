// test_nested_matches
fn main() {
    assert test_func(0, 0) == 6;
    assert test_func(1, 0) == 3;
    return;
}

fn test_func(a, b) -> 1 {
    x = 1;

    var mut_x_2;
    match a {
        0 => {
            var mut_x_1;
            mut_x_1 = x + 2;
            match b {
                0 => {
                    mut_x_2 = mut_x_1 + 3;
                }
            }
        }
        1 => {
            mut_x_2 = x + 2;
        }
    }

    return mut_x_2;
}
