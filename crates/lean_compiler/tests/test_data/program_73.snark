// test_mutable_edge_multiple_vars_different_branches
fn main() {
    mut a = 0;
    mut b = 0;
    mut c = 0;

    cond = 1;
    if cond == 1 {
        a = a + 1;
        b = b + 10;
        // c not modified
    } else {
        // a not modified
        b = b + 20;
        c = c + 100;
    }

    assert a == 1;
    assert b == 10;
    assert c == 0;
    return;
}
