const N = 10;

fn main() {
    arr = malloc(N);
    fill_array(arr);
    print_array(arr);
    return;
}

fn fill_array(arr) {
    for i in 0..N {
        if i == 0 {
            arr[i] = 10;
        } else if i == 1 {
            arr[i] = 20;
        } else if i == 2 {
            arr[i] = 30;
        } else {
            i_plus_one = i + 1;
            arr[i] = i_plus_one;
        }
    }
    return;
}

fn print_array(arr) {
    for i in 0..N {
        arr_i = arr[i];
        print(arr_i);
    }
    return;
}
